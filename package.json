{
  "name": "personal-site",
  "description": "Personal Web Site",
  "version": "2.2.11",
  "author": "martynov.max.s@gmail.com",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxmartynov/personal-site.git"
  },
  "homepage": "https://maxmartynov.com",
  "scripts": {
    "dev": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "NODE=production vue-cli-service build",
    "lint": "vue-cli-service lint",
    "inspect": "vue-cli-service inspect",
    "prod": "node build/server.prod.js",
    "bump": "node_modules/.bin/bump prompt --tag --all --push --commit 'v%s'",
    "patch": "node_modules/.bin/bump patch --tag --all --push --commit 'v%s'",
    "preversion": "echo 'Linting before bump...' && npm run lint",
    "budget": "node_modules/.bin/lighthouse https://maxmartynov.com/ ./public/budget.json --view",
    "firebase:login": "node_modules/.bin/firebase login",
    "firebase:deploy": "node_modules/.bin/firebase deploy",
    "deploy": "npm run build && npm run firebase:deploy"
  },
  "dependencies": {
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-gtag": "^1.6.2",
    "vue-i18n": "^8.10.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.1",
    "@vue/cli-plugin-eslint": "^4.4.1",
    "@vue/cli-plugin-pwa": "^4.4.1",
    "@vue/cli-service": "^4.5.13",
    "autodll-webpack-plugin": "^0.4.2",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.5.0",
    "dotenv": "^7.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.1",
    "fibers": "^4.0.2",
    "firebase-tools": "^11.22.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lighthouse": "^10.0.0",
    "serve-static": "^1.14.1",
    "version-bump-prompt": "^5.0.1",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "engines": {
    "node": "12.18.0",
    "npm": "6.14.7"
  }
}
